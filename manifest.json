{
  "manifest_version": 3,
  "name": "X User Notes - why did I block?",
  "version": "1.0",
  "icons": {
    "128": "img/128.png"
  },
  "description": "Keep track of your mute/block actions on X by saving original tweet link as a note attached to a user profile. Add private notes to every profile",
  "permissions": ["storage"],
  "key": "",
  "host_permissions": [
    "https://twitter.com/*",
    "https://x.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://x.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{a69a6479-ac77-468b-858b-69f05ccbc433}",
      "strict_min_version": "58.0"
    }
  },
  "options_page": "settings.html"
}
